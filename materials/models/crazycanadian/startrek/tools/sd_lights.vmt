"VertexlitGeneric"
{
	$basetexture "models/crazycanadian/startrek/tools/sd_lights"
	$basetexturetransform "center 1 1 scale 1 1 rotate 0 translate 0 0"
	"Proxies"
{
	"AnimatedTexture"
{
	"animatedTextureVar" "$basetexture"
	"animatedTextureFrameNumVar" "$frame"
	"animatedTextureFrameRate" "2"
}
}
	$selfillum 1
	
	$envmap env_cubemap
	$envmaptint "[ 0.1 0.1 0.1]"

	$phong 1
   	$phongexponent 10
	$phongboost 20
	$phongfresnelranges "[0 0.5 1]"
}

"VertexlitGeneric"
{
	$basetexture "crazycanadian/tools/sonicdriver/lights"
	$basetexturetransform "center 1 1 scale 1 1 rotate 0 translate 0 0"
	"Proxies"
{
	"AnimatedTexture"
{
	"animatedTextureVar" "$basetexture"
	"animatedTextureFrameNumVar" "$frame"
	"animatedTextureFrameRate" "60"
}
}
	$selfillum 1
	
	$envmap env_cubemap
	$envmaptint "[ 0.2 0.2 0.2]"

	$phong 1
   	$phongexponent 10
	$phongboost 20
	$phongfresnelranges "[0 0.5 1]"
}

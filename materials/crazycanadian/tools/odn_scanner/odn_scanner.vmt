"VertexlitGeneric"
{
	$basetexture "crazycanadian/tools/odn_scanner/scanner_diffuse"
	$normalmap	"crazycanadian/tools/odn_scanner/scanner_normal"
	
	$surfaceprop Plastic

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.1 0.1 0.1]"
	$envmapfresnel 0.9

	$phong 1
	$phongfresnelranges "[0 0.5 1]"
	$phongexponent 20
	$phongboost 5
}
